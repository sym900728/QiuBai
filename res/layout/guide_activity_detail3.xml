    android:layout_height="match_parent"
    android:background="#ffffff" >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:text="第三页"
        android:textColor="#000000"
        android:textSize="50dp" />

    <Button
        android:id="@+id/guide_enter_btn"
